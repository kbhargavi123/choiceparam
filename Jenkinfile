pipeline {
    agent any

    stages {
        stage('checkout scm') {
            steps {
               git branch: 'main', url: 'https://github.com/kbhargavi123/choiceparam.git'
                echo 'checkedout scm'
            }
        }
        stage('shell script execution') {
            steps {
                sh './param.sh $env' 
                
             
            }
        }
   }
}

       
